
<img src="/assets/img/burger.jpg" width="180" height="130">
<h1>Eat-Da-Burger!</h1>
<div id ="container">
<ul id ="ul">
  
  {{#each burgers}}
     {{#unless devoured}}
      {{> burgers/burger-block devoured=false}}
    {{/unless}}
  {{/each}}

</ul>


<ul id="ul1"> 
  
  {{#each burgers}}
    {{#if devoured}}
      {{> burgers/burger1-block devoured=true}}
    {{/if}}
  {{/each}}
</ul>


</div>

<br><br>
  <form class="create-form">
  <div class="form-group">
    
    <input type="text" id="burger" name="name">
    
  
  <br>
  <button type="submit" id="button"> Submit</button>
  <p id="paragraph">Enter a burger you want to eat Click the "Submit" button.</p>
  </div>
  </form>

